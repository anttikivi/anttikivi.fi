{
  "name": "@anttikivi/website",
  "version": "0.0.0",
  "private": true,
  "description": "Antti Kivi's website",
  "homepage": "https://github.com/anttikivi/anttikivi.github.io#readme",
  "bugs": {
    "url": "https://github.com/anttikivi/anttikivi.github.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttikivi/anttikivi.github.io.git"
  },
  "license": "Apache-2.0",
  "author": "Antti Kivi <antti@anttikivi.fi> (https://www.anttikivi.fi)",
  "type": "module",
  "scripts": {
    "build": "rm -rf _site && NODE_ENV=development eleventy",
    "build:production": "rm -rf _site && NODE_ENV=production eleventy",
    "build:staging": "rm -rf _site && NODE_ENV=staging eleventy",
    "dev": "NODE_ENV=development eleventy --serve",
    "format": "prettier --write \"**/*.{html,js?(on),md,njk}\"",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^5.0.0",
    "@eslint/js": "^9.16.0",
    "browserslist": "^4.24.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "lightningcss": "^1.28.2",
    "memoize": "^10.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-jinja-template": "^2.0.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4.0.0-beta.6",
    "tailwindcss-node-compiler": "^0.2.0"
  }
}
