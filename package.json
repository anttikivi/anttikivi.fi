{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "description": "Antti Kivi's personal website",
  "homepage": "https://github.com/anttikivi/website",
  "bugs": {
    "url": "https://github.com/anttikivi/website/issues"
  },
  "license": "Apache-2.0",
  "author": "Antti Kivi <antti.kivi@visiosto.fi> (https://www.anttikivi.fi)",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "format": "prettier --write \"**/*.{?([cm])[jt]s?(x),json,md?(x),?(s)css,html,astro,gitignore,env}\"",
    "format:test": "prettier --check \"**/*.{?([cm])[jt]s?(x),json,md?(x),?(s)css,html,astro,gitignore,env}\"",
    "lint": "concurrently 'npm:lint:*'",
    "lint:astro": "astro check",
    "lint:css": "stylelint \"src/styles/**/*.css\"",
    "lint:js": "eslint .",
    "lint:ts": "tsc --noEmit",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.3",
    "astro": "^4.0.3",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-astro-organize-imports": "^0.3.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "prettier-plugin-sh": "^0.13.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "rehype-external-links": "^3.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.3.3"
  }
}
